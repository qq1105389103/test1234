    </servlet>
    <servlet-mapping>
        <servlet-name>city_display</servlet-name>
        <url-pattern>/practice/city_display</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>city_displayAll</servlet-name>
        <url-pattern>/practice/city_displayAll</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>province_displayAllServlet</servlet-name>
        <url-pattern>/practice/province_displayAll</url-pattern>
