        <url-pattern>/shop_add</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>User_login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>User_findone</servlet-name>
        <url-pattern>/User/Server/find</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>User_findAll</servlet-name>
        <url-pattern>/User/Server/findAll</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>User_delete</servlet-name>
        <url-pattern>/User/Server/delete</url-pattern>
    </servlet-mapping>
    <listener>
        <listener-class>com.listener.listener_conn_create</listener-class>
    </listener>
    <!--以下是utf-8增强设置-->
    <filter>
        <filter-name>utf8_filter</filter-name>
        <filter-class>com.filter.filter_setutf8</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>utf8_filter</filter-name>
        <url-pattern>/User/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>utf8_filter</filter-name>
        <url-pattern>/question/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>utf8_filter</filter-name>
        <url-pattern>*.jsp</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>utf8_filter</filter-name>
        <url-pattern>/practice/*</url-pattern>
    </filter-mapping>
    <!--以下是登录检查-->
    <filter>
        <filter-name>login_check</filter-name>
        <filter-class>com.filter.login_check</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>login_check</filter-name>
        <url-pattern>/User/Server/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>login_check</filter-name>
        <url-pattern>/img/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>login_check</filter-name>
        <url-pattern>/question/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>login_check</filter-name>
        <url-pattern>/test/*</url-pattern>
    </filter-mapping>
</web-app>