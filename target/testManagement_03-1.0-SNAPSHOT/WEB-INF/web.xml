    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Question_destroy</servlet-name>
        <url-pattern>/question/delete</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Question_find</servlet-name>
        <url-pattern>/question/find</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Question_findAll</servlet-name>
        <url-pattern>/question/findAll</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Question_add</servlet-name>
        <url-pattern>/question/add</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login_out</servlet-name>
        <url-pattern>/out</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>shop_show</servlet-name>
        <url-pattern>/shop_show</url-pattern>
    </servlet-mapping>
