        <servlet-class>com.example.practice.bmi_comleteByAjaxServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>province_displayServlet</servlet-name>
        <servlet-class>com.example.practice.province_displayServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>province_displayAllServlet</servlet-name>
